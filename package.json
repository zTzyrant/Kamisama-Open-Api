{
	"name": "kamisama-open-api",
	"version": "1.0.50",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/index.ts",
		"build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts",
		"start": "bun run --watch src/index.ts",
		"auth:generate": "bun x @better-auth/cli generate --config src/libs/auth.ts",
		"auth:migrate": "bun x @better-auth/cli migrate",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "bun prisma/seed.ts",
		"db:push": "prisma db push"
	},
	"dependencies": {
		"@better-auth/cli": "^1.2.12",
		"@elysiajs/bearer": "^1.3.0",
		"@elysiajs/cors": "^1.3.3",
		"@elysiajs/jwt": "^1.3.1",
		"@elysiajs/static": "^1.3.0",
		"@elysiajs/swagger": "^1.3.1",
		"@prisma/client": "^6.11.1",
		"@types/crypto-js": "^4.2.2",
		"@types/nodemailer": "^6.4.17",
		"axios": "^1.10.0",
		"better-auth": "^1.2.12",
		"crypto-js": "^4.2.0",
		"elysia": "latest",
		"nodemailer": "^7.0.5",
		"prisma": "^6.11.1",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"bun-types": "latest"
	},
	"module": "src/index.js"
}
